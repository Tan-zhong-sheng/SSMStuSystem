<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="mapper.Schoolroom">
  		<insert id="Scinsert" useGeneratedKeys="true" keyProperty="Scid">
  			insert into schoolroom(RoomAddress,Management) values(#{RoomAddress},#{Management})
  		</insert>
  		
  		<select id="SelectSchoolRoom" resultType="pojo.SchoolRoom">
  			select * from schoolroom limit #{0},#{1}
  		</select>
  		
  		<select id="Count" resultType="Integer">
  			select count(*) from schoolroom
  		</select>
  		
  		<delete id="DeleteSchoolRoom">
  			delete from schoolroom where Scid=#{Scid}
  		</delete>
  		
  		<update id="UpdateSchoolRoom">
  			update schoolroom set Management = #{1} where Scid = #{0}
  		</update>
  </mapper>